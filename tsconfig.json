{
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es2016",
    "module": "esnext",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "lib": ["es6", "DOM"],
    "paths": {
      "@mini-vue3/*":["src/*/src"]
    }
  },
  "include": ["src/**/*.ts"]
}
